   </variant>
   <variant name="carousel-grid">
      <label>[Carousel] Grid: Basic</label>
      <selectable>true</selectable>
   </variant>
   <variant name="carousel-grid-detailed">
      <label>[Carousel] Grid: Detailed</label>
      <selectable>true</selectable>
   </variant>
   <variant name="carousel-grid-detailed-flipped">
      <label>[Carousel] Grid: Detailed - Flipped</label>
      <selectable>true</selectable>
